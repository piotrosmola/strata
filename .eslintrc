{
  "root": true,
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "jquery": true,
    "browser": true,
    "node": true,
    "es6": true,
    "amd": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:import/recommended"],
  "rules": {
    "space-before-function-paren": "off",
    "no-unused-vars": "off",
    "prefer-const": "warn",
    "no-extra-parens": "warn"
  }
}
